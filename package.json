{
  "name": "react-spa-starter",
  "version": "1.0.0",
  "description": "A boilerplate app to get started with react, redux & webpack2",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "set NODE_ENV=development && npm run clean && webpack --config ./webpack.config.dev.js",
    "build:prod": "set NODE_ENV=production && npm run clean && webpack -p --config ./webpack.config.prod.js",
    "start": "webpack-dev-server --config ./webpack.config.dev.js",
    "server": "set NODE_ENV=development && node server.js",
    "server:prod": "set NODE_ENV=production && node server.js"
  },
  "repository": "https://github.com/StephenGrider/WebpackProject",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.24.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.0",
    "npm": "^4.2.0",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.0",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
